<%- userFields = ["name", "email", "created_at", "updated_at", "phone_number"] -%>
<%- facilityFields = ["name", "welcomes", "services", "address", "phone", "website", "created_at", "updated_at"] -%>
<%= CSV.generate_line userFields %>
<%= CSV.generate_line facilityFields %>
<%- @users.each do |u| -%>
<%= CSV.generate_line([u.name, u.email, u.created_at, u.updated_at, u.phone_number]) -%>
<%- end -%>
<%= @facilities.each do |f| -%>
<%= CSV.generate_line([f.name, f.welcomes, f.services, f.address, f.phone, f.website, f.created_at, f.updated_at]) -%>
<%- end -%>
